{
  "name": "hyblock-scraper",
  "description": "dworac's hyblock scraper and discord bot for automatic updates",
  "version": "1.0.0",
  "author": "dworac <mail@dworac.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn esbuild:prod",
    "build:dev": "yarn clean && yarn esbuild:dev",
    "esbuild:dev": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "esbuild:prod": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --sourcemap --minify",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+template-repository.git"
  },
  "bugs": {
    "url": "https://github.com/dworac/node-hyblock-bot/issues"
  },
  "homepage": "https://github.com/dworac/node-hyblock-bot/#readme",
  "devDependencies": {
    "@dworac/eslint-config-typescript": "^1.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "esbuild": "^0.16.12",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@dworac/logger": "^1.1.0"
  }
}
